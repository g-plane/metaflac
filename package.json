{
  "name": "@gplane/metaflac",
  "description": "Library for processing metadata of FLAC files.",
  "author": "Pig Fang <g-plane@hotmail.com>",
  "repository": "g-plane/metaflac",
  "version": "0.0.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p . --outDir dist/esm && tsc -p . --outDir dist/cjs --module commonjs",
    "prepublishOnly": "pnpm build",
    "fmt": "prettier --write src tests",
    "fmt:check": "prettier --check src tests",
    "test": "vitest"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@gplane/tsconfig": "^5.0.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vitest": "^0.29.7"
  }
}
